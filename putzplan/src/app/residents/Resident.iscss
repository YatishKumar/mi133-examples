@import './src/app/util/common';

.residentContainer {
  background: $green;
  padding: $normalGap;
}

.innerContainer {
  width: 100%;
  display: flex;
  flex-direction: row;
}

.nameRow {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.tasksButton {
  background: #99ccff;
  width: $buttonHeight;
  height: $buttonHeight;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.gap {
  flex-grow: 100;
}

